FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      kinematicCloudProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

kinematicCloud
{
    injectionModels
    (
        patchInjection
        {
            type            patchInjection;
            patchName       inlet;
            U0              1.0;
            duration        5.0;
            parcelsPerSecond 5000;
            
            sizeDistribution
            {
                type        logNormal;
                median      2.5e-06;
                sigma       0.4;
            }
        }
    );

    particleProperties
    {
        rho             1000;
    }
}

